{
	"icon": "img/FXML-Viewer.png",
	"name": "fxml-viewer",
	"displayName": "FXML Viewer",
	"description": "A vscode extension view FXML file in SceneBuilder",
	"version": "1.0.1",
	"publisher": "lzw-723",
	"author": "Zhiwei Li",
	"license": "WTFPL",
	"readme": "README.md",
	"bugs": "https://github.com/lzw-723/FXML-Viewer/issues",
	"repository": "https://github.com/lzw-723/FXML-Viewer",
	"engines": {
		"vscode": "^1.42.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:fxml-viewer.openFXML"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fxml-viewer.openFXML",
				"title": "Open FXML",
				"icon": {
					"light": "./img/view_light.svg",
					"dark": "./img/view_dark.svg"
				}
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "fxml-viewer.openFXML",
					"when": "resourceExtname == .fxml",
					"group": "navigation"
				}
			],
			"commandPalette": [
				{
					"command": "fxml-viewer.openFXML",
					"when": "resourceExtname == .fxml"
				}
			],
			"explorer/context": [
				{
					"command": "fxml-viewer.openFXML",
					"when": "resourceExtname == .fxml"
				}
			],
			"editor/context": [
				{
					"command": "fxml-viewer.openFXML",
					"when": "resourceExtname == .fxml"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "FXML Viewer",
			"properties": {
				"fxml-viewer.SceneBuilder.executable": {
					"type": "string",
					"default": null,
					"description": "SceneBuilder executable file path"
				},
				"fxml-viewer.autoFindPath": {
					"type":"boolean",
					"default":"true",
					"description": "Whether to automatically search the executable file path of SceneBuilder(if you not set SceneBuilder executable file path)"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"keywords": [
		"java",
		"JavaFx",
		"fxml",
		"ScenceBuilder"
	],
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"regedit": "^3.0.3"
	}
}
